{
  "plugins": ["angular"],
  "extends": "angular",
  "rules": {
    "angular/deferred": 2,
    "angular/no-service-method": 0,
    "angular/window-service": 0,
    "array-bracket-spacing": 2,
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "brace-style": 2,
    "callback-return": 2,
    "camelcase": 2,
    "comma-dangle": 2,
    "comma-style": 2,
    "complexity": [2, 5],
    "constructor-super": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "indent": [2, 2, { "MemberExpression": "off" } ],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": 2,
    "max-len": [2, 120],
    "new-parens": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-const-assign": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": [2, { "skipBlankLines": false }],
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "object-curly-spacing": [2, "always"],
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-rest-params": 2,
    "quotes": [2, "double"],
    "require-yield": 2,
    "semi": [2, "always"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "use-isnan": 2,
    "valid-typeof": 2,
    "yoda": [2, "never"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "expect": true,
    "assert": true,
    "sinon": true
  }
}
