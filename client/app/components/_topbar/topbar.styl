topbar > md-toolbar
  height 42px
  max-height 42px
  min-height 42px
  margin-bottom: 15px

@media screen and (max-width: 600px)
  topbar > md-toolbar
    margin-bottom: 0px

topbar a.no-transform
  text-transform: none
  margin: 0

.breadcrumb
    color: grey
    padding: .4em
    font-size: 0.9em

.breadcrumb a
    color: grey
    text-decoration: none;
