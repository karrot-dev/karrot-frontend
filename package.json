{
  "private": true,
  "name": "karrot-frontend",
  "productName": "karrot",
  "version": "4.0.0",
  "description": "Modern website for organization of foodsaving groups worldwide",
  "author": "Foodsaving Worldwide Dev Team <foodsaving@yunity.org>",
  "homepage": "https://karrot.world/",
  "license": "MIT",
  "keywords": [
    "foodsharing",
    "foodsaving",
    "community",
    "organization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yunity/karrot-frontend"
  },
  "bugs": "https://github.com/yunity/karrot-frontend/issues",
  "scripts": {
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js",
    "build:cordova": "CORDOVA=true node build/script.build.js",
    "serve": "node build/script.serve.js",
    "test": "node test/jestUTC.js",
    "test:update": "node test/jestUTC.js --verbose --updateSnapshot",
    "test:watch": "node test/jestUTC.js --verbose --watchAll --notify",
    "e2e": "node e2e/testcafe.js",
    "lint": "eslint --ext .js,.vue src",
    "fix": "eslint --ext .js,.vue --fix src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "update_i18n": "python update_i18n.py",
    "docs": "gitbook install docs && gitbook serve docs",
    "build-docs": "gitbook install docs && gitbook build docs docs-dist"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-runtime": "^6.25.0",
    "clone-deep": "^2.0.1",
    "date-fns": "^1.29.0",
    "deep-equal": "^1.0.1",
    "deepmerge": "^2.0.1",
    "eslint-plugin-vue": "4.0.0-beta.2",
    "intl": "^1.2.5",
    "jstimezonedetect": "^1.0.6",
    "leaflet.awesome-markers": "^2.0.4",
    "loglevel": "^1.6.0",
    "markdown-it": "^8.4.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-link-attributes": "^2.0.0",
    "quasar-extras": "0.x",
    "quasar-framework": "^0.14.1",
    "raven-js": "^3.20.1",
    "reconnecting-websocket": "^3.2.1",
    "twemoji": "^2.5.0",
    "typeface-cabin-sketch": "^0.0.44",
    "vue": "~2.5.9",
    "vue-i18n": "^7.2.0",
    "vue-router": "^3.0.1",
    "vue2-leaflet": "^0.0.57",
    "vuelidate": "^0.6.0",
    "vuex": "^3.0.0",
    "vuex-connect": "^2.0.0",
    "vuex-persistedstate": "^2.0.0",
    "vuex-router-sync": "^5.0.0",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-links": "^3.2.17",
    "@storybook/vue": "^3.2.17",
    "autoprefixer": "^7.2.2",
    "axios-mock-adapter": "^1.10.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.0.0",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.1.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "gitbook-cli": "^2.3.2",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.0",
    "jest": "^21.2.1",
    "jest-junit": "^3.4.0",
    "jest-localstorage-mock": "^2.1.0",
    "jest-serializer-vue": "^0.3.0",
    "jest-vue-preprocessor": "^1.1.0",
    "json-loader": "^0.5.7",
    "lolex": "^2.3.0",
    "opn": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "purify-css": "^1.2.5",
    "raf": "^3.4.0",
    "shelljs": "^0.7.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "testcafe": "^0.18.4",
    "testcafe-browser-provider-saucelabs": "^1.3.0",
    "url-loader": "^0.6.2",
    "vue-loader": "~13.5.0",
    "vue-server-renderer": "^2.5.11",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "~2.5.9",
    "vue-test-utils": "1.0.0-beta.8",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      ">/(.*)$": "<rootDir>/test/$1",
      "^quasar$": "<rootDir>/node_modules/quasar-framework",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "jest-vue-preprocessor"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@storybook|quasar-framework))"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/setup/unhandledPromiseRejectionHandler.js",
      "<rootDir>/test/setup/mockRandom.js",
      "jest-localstorage-mock"
    ],
    "globals": {
      "CORDOVA": false
    }
  }
}
