{
  "private": true,
  "name": "karrot-frontend",
  "productName": "Karrot",
  "version": "8.8.1",
  "description": "Modern website for organization of foodsaving groups worldwide",
  "author": "Karrot Team <info@karrot.world>",
  "homepage": "https://karrot.world/",
  "license": "MIT",
  "keywords": [
    "foodsharing",
    "foodsaving",
    "community",
    "organization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yunity/karrot-frontend"
  },
  "bugs": "https://github.com/yunity/karrot-frontend/issues",
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build -m pwa",
    "build:cordova:dev": "cross-env KARROT_THEME=dev BACKEND=https://dev.karrot.world quasar build -m android --skip-pkg",
    "build:cordova:prod": "cross-env BACKEND=https://karrot.world quasar build -m android --skip-pkg",
    "serve": "node build/script.serve.js",
    "test": "cross-env TZ=UTC node test/jestUTC.js",
    "test:update": "cross-env TZ=UTC node test/jestUTC.js --verbose --updateSnapshot",
    "test:watch": "cross-env TZ=UTC node test/jestUTC.js --verbose --watchAll --notify",
    "lint": "eslint --ext .js,.vue ./",
    "fix": "npm run lint -- --fix",
    "lint:style": "stylelint --custom-syntax ./build/stylelintCustomSyntax.js 'src/**/*.{vue,styl}'",
    "fix:style": "npm run lint:style -- --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "locales:pull": "updateLocalesHelper/env/bin/python updateLocalesHelper/updateLocaleStatus.py && updateLocalesHelper/env/bin/tx pull -a --force --no-interactive",
    "locales:push": "updateLocalesHelper/env/bin/tx push -s",
    "docs": "gitbook install docs && gitbook serve docs",
    "build-docs": "gitbook install docs && gitbook build docs docs-dist"
  },
  "dependencies": {
    "@quasar/extras": "^1.9.7",
    "@sentry/browser": "^5.25.0",
    "@sentry/integrations": "^5.25.0",
    "axios": "^0.20.0",
    "clone-deep": "^4.0.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "deep-equal": "^2.0.4",
    "deepmerge": "^4.2.2",
    "element-closest": "^3.0.0",
    "escape-string-regexp": "^4.0.0",
    "firebase": "^7.23.0",
    "intl": "^1.2.5",
    "jstimezonedetect": "^1.0.7",
    "leaflet": "^1.7.1",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.7.0",
    "markdown-it": "^11.0.1",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-link-attributes": "^3.0.0",
    "quasar": "^1.14.1",
    "reconnecting-websocket": "^4.4.0",
    "twemoji": "^13.0.1",
    "vue": "^2.6.12",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.22.0",
    "vue-router": "^3.4.6",
    "vue2-leaflet": "2.5.2",
    "vuelidate": "^0.7.4",
    "vuex": "^3.5.1",
    "vuex-connect": "^2.0.1",
    "vuex-persistedstate": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@octokit/rest": "^18.0.6",
    "@quasar/app": "^2.1.1",
    "@quasar/quasar-app-extension-dotenv": "^1.0.5",
    "@sentry/cli": "^1.58.0",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/vue": "^6.0.26",
    "@types/jest": "^26.0.14",
    "@vue/test-utils": "^1.1.1",
    "axios-mock-adapter": "^1.18.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.5.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.6.0",
    "cordova-config": "^0.7.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.0.1",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "gitbook-cli": "^2.3.2",
    "glob": "^7.1.6",
    "hard-source-webpack-plugin": "^0.13.1",
    "http-proxy-middleware": "^1.0.5",
    "jest": "^26.5.2",
    "jest-canvas-mock": "^2.3.0",
    "jest-environment-jsdom": "^26.5.2",
    "jest-junit": "^12.0.0",
    "jest-serializer-vue": "^2.0.2",
    "jstoxml": "^1.6.11",
    "keep-a-changelog": "^0.10.1",
    "lodash": "^4.17.20",
    "lolex": "^6.0.0",
    "postcss-styl": "^0.7.2",
    "postcss-syntax": "^0.36.2",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "puppeteer": "^5.3.1",
    "raf": "^3.4.0",
    "shx": "^0.3.2",
    "stylelint": "^13.7.2",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "vue-jest": "^3.0.7",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "webpack-bundle-analyzer": "^3.9.0",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^quasar$": "<rootDir>/node_modules/quasar/dist/quasar.common.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less|styl|stylus)$": "<rootDir>/test/styleMock.js",
      "@/(.*)$": "<rootDir>/src/$1",
      ">/(.*)$": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!quasar/lang)"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "pretendToBeVisual": true
    },
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/setup/env.js",
      "<rootDir>/test/setup/unhandledPromiseRejectionHandler.js",
      "<rootDir>/test/setup/mockRandom.js",
      "<rootDir>/test/setup/mockScroll.js",
      "<rootDir>/test/setup/mockUserAgent.js",
      "<rootDir>/test/setup/mockLocation.js",
      "jest-canvas-mock"
    ]
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
