{
  "private": true,
  "name": "karrot-frontend",
  "productName": "karrot",
  "version": "8.5.1",
  "description": "Modern website for organization of foodsaving groups worldwide",
  "author": "Foodsaving Worldwide Dev Team <foodsaving@yunity.org>",
  "homepage": "https://karrot.world/",
  "license": "MIT",
  "keywords": [
    "foodsharing",
    "foodsaving",
    "community",
    "organization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yunity/karrot-frontend"
  },
  "bugs": "https://github.com/yunity/karrot-frontend/issues",
  "scripts": {
    "clean": "shx rm -rf dist",
    "dev": "webpack-dev-server --hot --inline --config build/webpack.config.js",
    "build": "npm run clean && npm run build:main && npm run build:statics && npm run build:service-worker",
    "build:main": "cross-env NODE_ENV=production webpack",
    "build:statics": "shx cp -r src/statics src/maintenance.html dist/",
    "build:service-worker": "cross-env NODE_ENV=production webpack --config build/webpack.service-worker.conf.js",
    "build:cordova:dev": "npm run clean && cross-env NODE_ENV=production KARROT_THEME=dev BACKEND=https://dev.karrot.world CORDOVA=true webpack && npm run build:statics",
    "build:cordova:prod": "npm run clean && cross-env NODE_ENV=production BACKEND=https://karrot.world CORDOVA=true webpack && npm run build:statics",
    "serve": "node build/script.serve.js",
    "test": "cross-env TZ=UTC node test/jestUTC.js",
    "test:update": "cross-env TZ=UTC node test/jestUTC.js --verbose --updateSnapshot",
    "test:watch": "cross-env TZ=UTC node test/jestUTC.js --verbose --watchAll --notify",
    "lint": "eslint --ext .js,.vue build scripts src test",
    "fix": "npm run lint -- --fix",
    "lint:style": "stylelint --custom-syntax ./build/stylelintCustomSyntax.js 'src/**/*.{vue,styl}'",
    "fix:style": "npm run lint:style -- --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "locales:pull": "updateLocalesHelper/env/bin/python updateLocalesHelper/updateLocaleStatus.py && updateLocalesHelper/env/bin/tx pull -a --force --no-interactive",
    "locales:push": "updateLocalesHelper/env/bin/tx push -s",
    "docs": "gitbook install docs && gitbook serve docs",
    "build-docs": "gitbook install docs && gitbook build docs docs-dist"
  },
  "dependencies": {
    "@quasar/extras": "^1.7.0",
    "@sentry/browser": "^5.10.2",
    "@sentry/integrations": "^5.10.2",
    "axios": "^0.19.0",
    "clone-deep": "^4.0.1",
    "date-fns": "^2.14.0",
    "deep-equal": "^2.0.1",
    "deepmerge": "^4.2.2",
    "element-closest": "^3.0.0",
    "escape-string-regexp": "^4.0.0",
    "firebase": "^7.14.5",
    "intl": "^1.2.5",
    "jstimezonedetect": "^1.0.7",
    "leaflet": "^1.6.0",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.6.6",
    "markdown-it": "^11.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-link-attributes": "^3.0.0",
    "quasar": "^1.11.3",
    "reconnecting-websocket": "^4.4.0",
    "twemoji": "^13.0.0",
    "vue": "^2.6.11",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.17.7",
    "vue-router": "^3.2.0",
    "vue2-leaflet": "2.5.2",
    "vuelidate": "^0.7.4",
    "vuex": "^3.3.0",
    "vuex-connect": "^2.0.1",
    "vuex-persistedstate": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "7.9.6",
    "@octokit/rest": "^17.9.2",
    "@sentry/cli": "^1.53.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "@types/jest": "^25.2.3",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^9.8.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-vue": "^2.0.2",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.6.0",
    "cordova-config": "^0.7.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.0",
    "css-loader": "^3.3.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "file-saver": "^2.0.2",
    "gitbook-cli": "^2.3.2",
    "glob": "^7.1.6",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.3.0",
    "http-proxy-middleware": "^1.0.4",
    "jest": "^26.0.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-junit": "^10.0.0",
    "jest-serializer-vue": "^2.0.2",
    "json-loader": "^0.5.7",
    "jstoxml": "^1.6.7",
    "keep-a-changelog": "^0.10.1",
    "koa-connect": "^2.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "lolex": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-html": "^0.36.0",
    "postcss-loader": "^3.0.0",
    "postcss-styl": "^0.7.1",
    "postcss-syntax": "^0.36.2",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "puppeteer": "^3.1.0",
    "raf": "^3.4.0",
    "shx": "^0.3.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.5.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-webpack-plugin": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^3.0.0",
    "url-loader": "^4.1.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.2",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "browserslist": [
    "defaults"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less|styl|stylus)$": "<rootDir>/test/styleMock.js",
      "@/(.*)$": "<rootDir>/src/$1",
      ">/(.*)$": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!quasar)"
    ],
    "testEnvironment": "jest-environment-jsdom-fifteen",
    "testEnvironmentOptions": {
      "pretendToBeVisual": true
    },
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/setup/unhandledPromiseRejectionHandler.js",
      "<rootDir>/test/setup/mockRandom.js",
      "<rootDir>/test/setup/mockScroll.js",
      "<rootDir>/test/setup/mockUserAgent.js",
      "<rootDir>/test/setup/mockLocation.js",
      "jest-canvas-mock"
    ],
    "globals": {
      "__ENV": {
        "CORDOVA": false,
        "DEV": true,
        "KARROT_THEME": "dev"
      }
    }
  }
}
