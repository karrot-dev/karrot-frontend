{
  "private": true,
  "name": "karrot-frontend",
  "productName": "Karrot",
  "version": "9.4.0",
  "description": "Karrot is a free and open-source tool for grassroots initiatives and groups of people that want to coordinate face-to-face activities on a local, autonomous and voluntary basis.",
  "author": "Karrot Team <info@karrot.world>",
  "homepage": "https://karrot.world/",
  "license": "MIT",
  "keywords": [
    "foodsharing",
    "foodsaving",
    "community",
    "organization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yunity/karrot-frontend"
  },
  "bugs": "https://github.com/yunity/karrot-frontend/issues",
  "scripts": {
    "dev": "quasar dev",
    "dev:backend:dev": "cross-env BACKEND=https://dev.karrot.world quasar dev",
    "dev:backend:local": "cross-env BACKEND=http://localhost:8000 quasar dev",
    "build": "quasar build -m pwa",
    "build:cordova:dev": "cross-env KARROT_THEME=dev BACKEND=https://dev.karrot.world quasar build -m android --skip-pkg",
    "build:cordova:prod": "cross-env BACKEND=https://karrot.world quasar build -m android --skip-pkg",
    "serve": "node build/script.serve.js",
    "test": "cross-env TZ=UTC node test/jestUTC.js",
    "test:update": "cross-env TZ=UTC node test/jestUTC.js --verbose --updateSnapshot",
    "test:watch": "cross-env TZ=UTC node test/jestUTC.js --verbose --watchAll --notify",
    "lint": "eslint --ext .js,.vue ./",
    "fix": "npm run lint -- --fix",
    "lint:style": "stylelint --custom-syntax ./build/stylelintCustomSyntax.js 'src/**/*.{vue,styl}'",
    "fix:style": "npm run lint:style -- --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "locales:pull": "updateLocalesHelper/env/bin/python updateLocalesHelper/updateLocaleStatus.py && updateLocalesHelper/env/bin/tx pull -a --force --no-interactive",
    "locales:push": "updateLocalesHelper/env/bin/tx push -s",
    "docs": "cd docs && mdbook serve",
    "build-docs": "cd docs && mdbook build --dest-dir ../docs-dist",
    "generate-landing-images": "node src/base/generate-images.mjs"
  },
  "dependencies": {
    "@quasar/extras": "^1.10.7",
    "@sentry/browser": "^6.7.1",
    "@sentry/integrations": "^6.7.1",
    "axios": "^0.21.1",
    "clone-deep": "^4.0.1",
    "core-js": "^3.14.0",
    "date-fns": "^2.22.1",
    "deep-equal": "^2.0.5",
    "deepmerge": "^4.2.2",
    "element-closest": "^3.0.0",
    "escape-string-regexp": "^5.0.0",
    "firebase": "^8.6.8",
    "intl": "^1.2.5",
    "jstimezonedetect": "^1.0.7",
    "leaflet": "^1.7.1",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.7.1",
    "markdown-it": "^12.0.6",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "quasar": "^1.15.21",
    "reconnecting-websocket": "^4.4.0",
    "twemoji": "^13.1.0",
    "vue": "^2.6.14",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.24.5",
    "vue-router": "^3.5.1",
    "vue2-leaflet": "2.7.0",
    "vuelidate": "^0.7.6",
    "vuex": "^3.6.2",
    "vuex-connect": "^2.0.1",
    "vuex-persistedstate": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@mapbox/appropriate-images": "^4.1.0",
    "@octokit/rest": "^18.6.0",
    "@quasar/app": "^2.2.10",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "@quasar/quasar-ui-qiconpicker": "^1.3.3",
    "@sentry/cli": "^1.66.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/vue": "^6.2.9",
    "@types/jest": "^26.0.23",
    "@vue/test-utils": "^1.2.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^27.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.6.0",
    "cordova-config": "^0.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.11.1",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "glob": "^7.1.7",
    "hard-source-webpack-plugin": "^0.13.1",
    "http-proxy-middleware": "^2.0.0",
    "jest": "^27.0.4",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom": "^27.0.3",
    "jest-junit": "^12.2.0",
    "jest-serializer-vue": "^2.0.2",
    "jstoxml": "^2.0.6",
    "keep-a-changelog": "^0.10.4",
    "lodash": "^4.17.21",
    "lolex": "^6.0.0",
    "postcss-styl": "^0.8.0",
    "postcss-syntax": "^0.36.2",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "puppeteer": "^10.0.0",
    "raf": "^3.4.0",
    "shx": "^0.3.3",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-webpack-plugin": "^2.2.0",
    "vue-jest": "^3.0.7",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack-bundle-analyzer": "^4.4.2",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^quasar$": "<rootDir>/node_modules/quasar/dist/quasar.common.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less|styl|stylus)$": "<rootDir>/test/styleMock.js",
      "@/(.*)$": "<rootDir>/src/$1",
      ">/(.*)$": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!quasar/lang|escape-string-regexp)"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "pretendToBeVisual": true
    },
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/setup/env.js",
      "<rootDir>/test/setup/unhandledPromiseRejectionHandler.js",
      "<rootDir>/test/setup/mockRandom.js",
      "<rootDir>/test/setup/mockScroll.js",
      "<rootDir>/test/setup/mockUserAgent.js",
      "<rootDir>/test/setup/mockLocation.js",
      "jest-canvas-mock"
    ]
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
