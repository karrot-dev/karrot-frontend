{
  "private": true,
  "name": "karrot-frontend",
  "productName": "karrot",
  "version": "7.4.2",
  "description": "Modern website for organization of foodsaving groups worldwide",
  "author": "Foodsaving Worldwide Dev Team <foodsaving@yunity.org>",
  "homepage": "https://karrot.world/",
  "license": "MIT",
  "keywords": [
    "foodsharing",
    "foodsaving",
    "community",
    "organization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yunity/karrot-frontend"
  },
  "bugs": "https://github.com/yunity/karrot-frontend/issues",
  "scripts": {
    "clean": "shx rm -rf dist",
    "dev": "webpack-dev-server --hot --inline --config build/webpack.config.js",
    "build": "npm run clean && npm run build:main && npm run build:statics && npm run build:service-worker",
    "build:main": "NODE_ENV=production webpack",
    "build:statics": "shx cp -r src/statics src/maintenance.html dist/",
    "build:service-worker": "NODE_ENV=production webpack --config build/webpack.service-worker.conf.js",
    "build:cordova:dev": "npm run clean && NODE_ENV=production KARROT_THEME=dev BACKEND=https://dev.karrot.world CORDOVA=true webpack && npm run build:statics",
    "build:cordova:prod": "npm run clean && NODE_ENV=production BACKEND=https://karrot.world CORDOVA=true webpack && npm run build:statics",
    "serve": "node build/script.serve.js",
    "test": "TZ=UTC node test/jestUTC.js",
    "test:update": "TZ=UTC node test/jestUTC.js --verbose --updateSnapshot",
    "test:watch": "TZ=UTC node test/jestUTC.js --verbose --watchAll --notify",
    "lint": "eslint --ext .js,.vue build scripts src test",
    "fix": "npm run lint -- --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "locales:pull": "updateLocalesHelper/env/bin/python updateLocalesHelper/updateLocaleStatus.py && updateLocalesHelper/env/bin/tx pull -a --force --no-interactive",
    "locales:push": "updateLocalesHelper/env/bin/tx push -s",
    "docs": "gitbook install docs && gitbook serve docs",
    "build-docs": "gitbook install docs && gitbook build docs docs-dist"
  },
  "dependencies": {
    "@quasar/extras": "^1.3.1",
    "@sentry/browser": "^5.6.3",
    "@sentry/integrations": "^5.6.1",
    "axios": "^0.19.0",
    "clone-deep": "4.0.1",
    "date-fns": "^2.0.1",
    "deep-equal": "^1.1.0",
    "element-closest": "^3.0.0",
    "escape-string-regexp": "^2.0.0",
    "firebase": "^6.5.0",
    "intl": "^1.2.5",
    "jstimezonedetect": "^1.0.6",
    "leaflet": "^1.5.1",
    "loglevel": "^1.6.3",
    "markdown-it": "^9.1.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-link-attributes": "^2.0.0",
    "quasar": "^1.1.0",
    "reconnecting-websocket": "^4.2.0",
    "twemoji": "^12.1.2",
    "typeface-cabin-sketch": "^0.0.72",
    "vue": "2.6.10",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.14.0",
    "vue-router": "^3.1.3",
    "vue2-leaflet": "2.2.1",
    "vuelidate": "^0.7.4",
    "vuex": "3.1.1",
    "vuex-connect": "^2.0.1",
    "vuex-persistedstate": "^2.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@babel/runtime-corejs3": "^7.5.5",
    "@octokit/rest": "^16.28.7",
    "@sentry/cli": "^1.47.1",
    "@storybook/addon-actions": "5.2.0-alpha.35",
    "@storybook/addon-links": "5.2.0-alpha.35",
    "@storybook/vue": "5.2.0-alpha.35",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-imports": "2.0.0",
    "babel-preset-vue": "^2.0.2",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.6.0",
    "cordova-config": "^0.7.0",
    "core-js": "3.2.1",
    "css-loader": "^3.2.0",
    "deepmerge": "^4.0.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "5.2.3",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "gitbook-cli": "^2.3.2",
    "glob": "^7.1.4",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.0.0-alpha",
    "http-proxy-middleware": "^0.19.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.1.1",
    "jest-environment-jsdom-fifteen": "^1.0.0",
    "jest-junit": "^8.0.0",
    "jest-serializer-vue": "^2.0.2",
    "json-loader": "^0.5.7",
    "keep-a-changelog": "^0.8.1",
    "koa-connect": "^2.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "lolex": "^4.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "raf": "^3.4.0",
    "shx": "^0.3.2",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^2.1.0",
    "vue-jest": "^3.0.3",
    "vue-loader": "^15.7.1",
    "vue-server-renderer": "2.6.10",
    "vue-template-compiler": "2.6.10",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "browserslist": [
    "defaults"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less|styl|stylus)$": "<rootDir>/test/styleMock.js",
      "@/(.*)$": "<rootDir>/src/$1",
      ">/(.*)$": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!quasar)"
    ],
    "testEnvironment": "jest-environment-jsdom-fifteen",
    "testEnvironmentOptions": {
      "pretendToBeVisual": true
    },
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/setup/unhandledPromiseRejectionHandler.js",
      "<rootDir>/test/setup/mockRandom.js",
      "<rootDir>/test/setup/mockScroll.js",
      "<rootDir>/test/setup/mockUserAgent.js",
      "<rootDir>/test/setup/mockLocation.js",
      "jest-canvas-mock"
    ],
    "globals": {
      "__ENV": {
        "CORDOVA": false,
        "DEV": true,
        "KARROT_THEME": "dev",
        "FCM_SENDER_ID": "test-fcm-sender-id"
      }
    }
  }
}
